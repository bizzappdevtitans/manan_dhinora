<odoo>
   <data>
       <record id="maintenance_cycle_email_template" model="mail.template">
           <field name="name">Maintenance Cycle Template</field>
           <field name="model_id" ref="model_account_move" />
           <field name="auto_delete" eval="True" />
           <field name="email_from">{{object.res_user_id.email}}</field>
           <field name="email_to">{{object.pratner_id.email}}</field>
           <field name="subject">{{object.amc}}</field>
           <field name="body_html">
               <p>
                   Dear<t t-out="object.pratner_id.name" />,
                   <br />
                   <br />
                   This is your reminder to get your fabricated vessle checkout, as you have signed up for our quaterly maintenance program.
                   <br />
               </p>
               Regards,
               <br />
               <t t-out="object.company_id.name" />
           </field>
       </record>
   </data>
</odoo>
